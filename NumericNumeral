package com.pillar.romannumeral.java;

/**
 * Created by kiran on 6/13/2015.
 */
public enum NumericNumeral {
    I(1), 
    
    IV(4), 
    
    V(5), 
    
    IX(9), 
    
    X(10), 
    
    XL(40), 
    
    L(50),
    
    XC(90), 
    
    C(100), 
    
    CD(400), 
    
    D(500), 
    
    CM(900), 
    
    M(1000);

    int num;

    private NumericNumeral(int num) {
        this.num =num;
    }
}
